{
  "name": "incois-ocean-hazard-reporting-platform",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "INCOIS Ocean Hazard Reporting Platform - Frontend Application",
  "author": "INCOIS Team",
  "license": "MIT",
  "keywords": [
    "ocean",
    "hazard",
    "reporting",
    "incois",
    "react",
    "typescript"
  ],
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build",
    "build:prod": "NODE_ENV=production vite build --mode production",
    "preview": "vite preview --host",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "clean": "rimraf dist",
    "analyze": "vite-bundle-analyzer"
  },
  "dependencies": {
    "@azure/storage-blob": "^12.28.0",
    "@capacitor/cli": "^7.4.3",
    "@capacitor/core": "^7.4.3",
    "@google/genai": "^1.20.0",
    "leaflet": "^1.9.4",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/leaflet": "^1.9.12",
    "@types/node": "^22.14.0",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.1",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "@vitejs/plugin-react": "^5.0.0",
    "eslint": "^9.15.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "rimraf": "^6.0.1",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "vite-bundle-analyzer": "^0.11.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
